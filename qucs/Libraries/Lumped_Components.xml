<Library name="Lumped Components">
    <Component name="Capacitor" schematic_id = "C" show_name="true">
        <Description>
            Ideal Capacitor
        </Description>
        <Netlists>
                <QucsatorNetlist value = 'C:C{PartCounter} {nets} C="{C}" V="{V}"'>
                </QucsatorNetlist>
                <NgspiceNetlist value = 'C{PartCounter} {nets} {C}'>
                </NgspiceNetlist>
        </Netlists>
        <Symbols>
            <Symbol id="Standard" type="explicit">
                <PortSym x="-30" y="0" type="1" angle="0" />
                <Line x1="-4" y1="-11" x2="-4" y2="11" color="#000080" width="2" style="1" />
                <Line x1="4" y1="-11" x2="4" y2="11" color="#000080" width="2" style="1" />
                <Line x1="-30" y1="0" x2="-4" y2="0" color="#000080" width="2" style="1" />
                <Line x1="4" y1="0" x2="30" y2="0" color="#000080" width="2" style="1" />
                <PortSym x="30" y="0" type="2" angle="180"/>
            </Symbol>
        </Symbols>
        <Parameters>
            <Parameter name="C" unit="pF" default_value="1" show="true">
                <Description>Capacitance</Description>
            </Parameter>
            <Parameter name="V" unit="V" default_value="1" show="false">
                <Description>Initial voltage for transient simulation</Description>
            </Parameter>
        </Parameters>
    </Component>
    <Component name="Resistor" schematic_id = "R" show_name="true">
        <Description>
            Ideal Resistor
        </Description>
        <Netlists>
                <QucsatorNetlist value = 'R:R{PartCounter} {nets} R="{R}" Temp="{Temp}" Tc1="{Tc1}" Tc2="{Tc2}"'>
                </QucsatorNetlist>
                <NgspiceNetlist value = 'R{PartCounter} {nets} {R} tc1={Tc1} tc1={Tc2}'>
                </NgspiceNetlist>
        </Netlists>
        <Symbols>
            <Symbol id="Standard" type="explicit">
                <PortSym x="-30" y="0" type="1" angle="0" />
                <Line x1="-18" y1="-9" x2="18" y2="-9" color="#000080" width="2" style="1" />
                <Line x1="18" y1="-9" x2="18" y2="9" color="#000080" width="2" style="1" />
                <Line x1="18" y1="9" x2="-18" y2="9" color="#000080" width="2" style="1" />
                <Line x1="-18" y1="9" x2="-18" y2="-9" color="#000080" width="2" style="1" />
                <Line x1="-30" y1="0" x2="-18" y2="0" color="#000080" width="2" style="1" />
                <Line x1="18" y1="0" x2="30" y2="0" color="#000080" width="2" style="1" />
                <PortSym x="30" y="0" type="2" angle="180"/>
            </Symbol>
        </Symbols>
        <Parameters>
            <Parameter name="R" unit="Ohm" default_value="1000" show="true">
                <Description>Resistance</Description>
            </Parameter>
            <Parameter name="Temp" unit="" default_value="26.85" show="false">
                <Description>Simulation temperature in degree Celsius (Qucsator only)</Description>
            </Parameter>
            <Parameter name="Tc1" unit="" default_value="0" show="false">
                <Description>First order temperature coefficient</Description>
            </Parameter>
            <Parameter name="Tc2" unit="" default_value="0" show="false">
                <Description>Second order temperature coefficient</Description>
            </Parameter>
            <Parameter name="Tnom" unit="" default_value="26.85" show="false">
                <Description>Temperature at which the parameters were extracted (Qucsator only)</Description>
            </Parameter>
        </Parameters>
    </Component>
    <Component name="Inductor" schematic_id = "L" show_name="true">
        <Description>
            Ideal Inductor
        </Description>
        <Netlists>
                <QucsatorNetlist value = 'L:L{PartCounter} {nets} L="{L}" I="{I}"'>
                </QucsatorNetlist>
                <NgspiceNetlist value = 'L{PartCounter} {nets} {L}'>
                </NgspiceNetlist>
        </Netlists>
        <Symbols>
            <Symbol id="Standard" type="external"> <!-- Defined externally only for future reference -->
               <File>./Lumped_Component_Symbols/Inductor.sym</File>
           </Symbol>
        </Symbols>
        <Parameters>
            <Parameter name="L" unit="nH" default_value="1" show="true">
                <Description>Inductance</Description>
            </Parameter>
            <Parameter name="I" unit="A" default_value="1e-3" show="false">
                <Description>Initial current for transient simulation</Description>
            </Parameter>
        </Parameters>
    </Component>
    <Component name="Ground" schematic_id = "GND" show_name="false">
        <Description>
            Ground
        </Description>
        <Models>
                <DefaultModel value = "GND">
                </DefaultModel>
                <SpiceModel value = "GND">
                </SpiceModel>
        </Models>
        <Symbols>
            <Symbol id="Standard" type="explicit">
                <PortSym x="0" y="0" type="1" angle="0" />
                <Line x1="0" y1="0" x2="0" y2="10" color="#000080" width="2" style="1" />
                <Line x1="-11" y1="10" x2="11" y2="10" color="#000080" width="3" style="1" />
                <Line x1="-7" y1="16" x2="7" y2="16" color="#000080" width="3" style="1" />
                <Line x1="-3" y1="22" x2="3" y2="22" color="#000080" width="3" style="1" />
           </Symbol>
        </Symbols>
    </Component>
</Library>
