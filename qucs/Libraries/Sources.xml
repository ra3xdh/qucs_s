<Library name="Sources">
    <Component name="AC Power Source" schematic_id = "Pac" show_name="true">
        <Description>
            AC Power Source
        </Description>
        <Netlists>
                <QucsatorNetlist value = 'Pac:P{PartCounter} {nets} Num="{PartCounter}" Z="{Z}" P="{P}" f="{f}" Temp="{Temp}"'>
                </QucsatorNetlist>
                <NgspiceNetlist value = 'VP{PartCounter} {nets} dc 0 ac {P} SIN(0 {P} {f}) portnum {Num} z0 {Z}'>
                </NgspiceNetlist>
        </Netlists>
        <Symbols>
            <Symbol id="Standard" type="explicit">
                <PortSym x="0" y="30" type="1" angle="270" />
                <Line x1="11" y1="-22" x2="11" y2="22" color="#35322F" width="0" style="1" /> <!-- Outer box -->
                <Line x1="-11" y1="-22" x2="-11" y2="22" color="#35322F" width="0" style="1" />
                <Line x1="11" y1="-22" x2="-11" y2="-22" color="#35322F" width="0" style="1" />
                <Line x1="11" y1="22" x2="-11" y2="22" color="#35322F" width="0" style="1" />
                <Line x1="0" y1="30" x2="0" y2="19" color="#000080" width="2" style="1" />
                <Line x1="0" y1="-30" x2="0" y2="-19" color="#000080" width="2" style="1" />
                <Line x1="0" y1="1" x2="0" y2="-3" color="#000080" width="2" style="1" />
                <Line x1="5" y1="-3" x2="5" y2="-19" color="#000080" width="2" style="1" />
                <Line x1="-5" y1="-3" x2="-5" y2="-19" color="#000080" width="2" style="1" />
                <Line x1="5" y1="-3" x2="-5" y2="-3" color="#000080" width="2" style="1" />
                <Line x1="5" y1="-19" x2="-5" y2="-19" color="#000080" width="2" style="1" />
                <Arc x="-9" y="1" arc_width="18" arc_height="18" angle="0" arclen="5760" color="#000080" pen_width="2" /> <!-- Circle -->
                <Arc x="-6" y="6" arc_width="6" arc_height="6" angle="0" arclen="2880" color="#000080" pen_width="2" /> <!-- Half sine -->
                <Arc x="0" y="6" arc_width="6" arc_height="6" angle="2880" arclen="2880" color="#000080" pen_width="2" /> <!-- The other half of the sine -->
                <Line x1="-5" y1="-25" x2="-11" y2="-25" color="#FF0000" width="1" style="1" /> <!-- Plus sign -->
                <Line x1="-8" y1="-28" x2="-8" y2="-22" color="#FF0000" width="1" style="1" />
                <Line x1="-5" y1="25" x2="-11" y2="25" color="#000000" width="1" style="1" /> <!-- Minus sign -->
                <PortSym x="0" y="-30" type="2" angle="90"/>
            </Symbol>
        </Symbols>
        <Parameters>
            <Parameter name="Num" unit="" default_value="1" show="true">
                <Description>Number of the port</Description>
            </Parameter>
            <Parameter name="Z" unit="Ohm" default_value="50" show="true">
                <Description>Port impedance</Description>
            </Parameter>
            <Parameter name="P" unit="dBm" default_value="0" show="true">
                <Description>(Available) AC power</Description>
            </Parameter>
            <Parameter name="f" unit="GHz" default_value="1" show="true">
                <Description>Frequency</Description>
            </Parameter>
            <Parameter name="Temp" unit="" default_value="26.85" show="false">
                <Description>Simulation temperature</Description>
            </Parameter>
        </Parameters>
    </Component>
    <Component name="DC Voltage Source" schematic_id = "Vdc" show_name="true">
        <Description>
            Ideal DC Voltage Source
        </Description>
        <Netlists>
                <QucsatorNetlist value = 'Vdc:V{PartCounter} {nets} U="{U}"'>
                </QucsatorNetlist>
                <NgspiceNetlist value = 'V{PartCounter} {nets} DC {U}'>
                </NgspiceNetlist>
        </Netlists>
        <Symbols>
            <Symbol id="Standard" type="explicit">
                <PortSym x="0" y="-30" type="2" angle="90"/>
                <Line x1="-13" y1="-4" x2="13" y2="-4" color="#14148C" width="2" style="1" />
                <Line x1="-6" y1="4" x2="6" y2="4" color="#14148C" width="4" style="1" />
                <Line x1="0" y1="-30" x2="0" y2="-4" color="#14148C" width="2" style="1" />
                <Line x1="0" y1="4" x2="0" y2="30" color="#14148C" width="2" style="1" />
                <Line x1="5" y1="-11" x2="11" y2="-11" color="#FF0000" width="1" style="1" /> <!-- Plus sign -->
                <Line x1="8" y1="-14" x2="8" y2="-8" color="#FF0000" width="1" style="1" />
                <Line x1="5" y1="11" x2="11" y2="11" color="#000000" width="1" style="1" /> <!-- Minus sign -->
                <PortSym x="0" y="30" type="1" angle="270" />
            </Symbol>
        </Symbols>
        <Parameters>
            <Parameter name="U" unit="V" default_value="1" show="true">
                <Description>Voltage</Description>
            </Parameter>
        </Parameters>
    </Component>
</Library>
